{
  "name": "@jafps/config",
  "version": "1.1.0",
  "description": "Functions for reading configuration.",
  "type": "module",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "test": "node --enable-source-maps --experimental-test-coverage --test-coverage-exclude \"src/**/__tests__/*.ts\" --test \"./dist/**/__tests__/*.{spec,test}.js\"",
    "lint": "eslint"
  },
  "author": "Joshua Avalon",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jafpsjs/config.git"
  },
  "bugs": {
    "url": "https://github.com/jafpsjs/config/issues"
  },
  "homepage": "https://github.com/jafpsjs/config#readme",
  "imports": {
    "#base": {
      "import": "./dist/base/index.js"
    },
    "#env": {
      "import": "./dist/env/index.js"
    },
    "#json": {
      "import": "./dist/json/index.js"
    },
    "#util": {
      "import": "./dist/util/index.js"
    },
    "#yaml": {
      "import": "./dist/yaml/index.js"
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./base": {
      "import": "./dist/base/index.js"
    },
    "./env": {
      "import": "./dist/env/index.js"
    },
    "./json": {
      "import": "./dist/json/index.js"
    },
    "./yaml": {
      "import": "./dist/yaml/index.js"
    }
  },
  "keywords": [
    "fastify"
  ],
  "files": [
    "dist/**/*",
    "!dist/**/__tests__/**/*"
  ],
  "devDependencies": {
    "@joshuaavalon/eslint-config-javascript": "^10.0.0",
    "@joshuaavalon/eslint-config-typescript": "^10.0.0",
    "@types/node": "^24.9.1",
    "eslint": "^9.38.0",
    "fastify": "^5.6.2",
    "globals": "^16.4.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  },
  "dependencies": {
    "mergician": "^2.0.2",
    "ts-extras": "0.16.0",
    "type-fest": "^5.2.0",
    "typebox": "^1.0.58",
    "yaml": "^2.8.2"
  }
}
